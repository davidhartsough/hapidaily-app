{
  "expo": {
    "name": "HapiDaily",
    "description": "have a positive impact daily",
    "slug": "hapidaily",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.0",
    "githubUrl": "https://github.com/davidhartsough/hapidaily-app",
    "orientation": "portrait",
    "primaryColor": "#1997FF",
    "icon": "./assets/images/hapidaily-ios-icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.davidhartsough.hapidaily",
      "buildNumber": "0.1.0",
      "icon": "./assets/images/hapidaily-ios-icon.png",
      "supportsTablet": true
    },
    "android": {
      "package": "com.davidhartsough.hapidaily",
      "versionCode": 1,
      "icon": "./assets/images/hapidaily-android-icon.png",
      "permissions": [
        "READ_CONTACTS"
      ]
    }
  }
}